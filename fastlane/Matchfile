git_url(ENV['MATCH_GIT_URL'])
git_branch(ENV["TEAM_ID"])                  

storage_mode(ENV['MATCH_STORAGE_MODE'])

type("appstore")                            
app_identifier ENV['MATCH_APP_IDENTIFIER']

api_key({
  key_id: ENV['MATCH_API_KEY_ID'],
  issuer_id: ENV['MATCH_ISSUER_ID'],
  filepath: ENV['MATCH_API_KEY_FILEPATH']
})
